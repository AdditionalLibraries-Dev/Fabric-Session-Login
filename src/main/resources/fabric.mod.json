{
	"schemaVersion": 1,
	"id": "session-id-login-mod",
	"version": "${version}",
	"name": "Session ID Login Mod",
	"description": "A lightweight Fabric client-side mod that adds a GUI to log in using Minecraft session IDs. Includes a status display (Valid/Invalid) and integrates cleanly into the multiplayer menu without affecting gameplay or server-side behavior.",
	"authors": [
		"Additional Libraries"
	],
	"contact": {
		"homepage": "https://discord.gg/PvzE7v5VHR",
		"sources": "https://github.com/AdditionalLibraries-Dev/Fabric-Session-Login"
	},
	"license": "CC0-1.0",
	"icon": "assets/session-id-login-mod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"dev.adlib.SessionIDLoginMod"
		]
	},
	"mixins": [
		"session-id-login-mod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.5",
		"minecraft": "~1.21.7",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}